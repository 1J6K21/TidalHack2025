{
  "name": "buildflow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:demo": "NEXT_PUBLIC_DEMO_MODE=true next dev",
    "dev:live": "NEXT_PUBLIC_DEMO_MODE=false next dev",
    "build": "next build",
    "build:staging": "NEXT_PUBLIC_APP_ENV=staging next build",
    "build:production": "NEXT_PUBLIC_APP_ENV=production next build",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "upload-demo-data": "node scripts/uploadDemoData.js",
    "setup-firebase": "echo 'Please configure Firebase project and upload demo data using: npm run upload-demo-data'"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "dotenv": "^17.2.3",
    "firebase": "^12.4.0",
    "framer-motion": "^12.23.24",
    "next": "16.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^4.0.3",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "jsdom": "^27.0.1",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.3"
  }
}
